var
nome_completo: caracter
cpf: caracter
datadenascimento: caracter
rg: inteiro
orgaoemissor: caracter
datadeexpedicao: caracter
nacionalidade: caracter
genero: caracter
acessibilidade: logico
cep: logico
logradouro: caracter
numero: inteiro
complemento: caracter
bairro: caracter
cidade: caracter
uf: caracter
email: caracter
celular: caracter

inicio
escreva("Digite seu nome ?")
leia (nome_completo)

escreva ("Digite seu cpf ?")
leia(cpf)

escreva("Digite sua data de nascimento ?")
leia(datadenascimento)

escreva("Digite seu rg ?")
leia(rg)

escreva("Qual seu orgaoemissor ?")
leia(orgaoemissor)

