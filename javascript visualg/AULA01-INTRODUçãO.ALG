escreva("Qual seu cep?")
leia(cep)

escreva("Onde você mora?")
leia(logradouro)

escreva("Qual numero da casa?")
leia(numero)

escreva("Complemento ?")
leia(complemento)

escreva("Qual seu bairro")
leia(bairro)

escreva(" Qual sua cidade")
leia(cidade)

escreva("Qual seu UF")
leia(uf)

